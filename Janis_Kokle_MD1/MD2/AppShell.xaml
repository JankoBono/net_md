<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MD2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MD2"
    Shell.FlyoutBehavior="Flyout"
    Title="MD2">

    <ShellContent
        Title="Datu menedžešana"
        ContentTemplate="{DataTemplate local:DatMainPage}"
        Route="DatMainPage" />
    <ShellContent
        Title="Studenta lapa"
        ContentTemplate="{DataTemplate local:NewPage1}"
        Route="NewPage1" />
    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->
    <ShellContent
        Title="Submission saraksts"
        ContentTemplate="{DataTemplate local:SubSaraksts}"
        Route="SubSaraksts" />
    <ShellContent
        Title="Assignment saraksts"
        ContentTemplate="{DataTemplate local:UzdSaraksts}"
        Route="UzdSaraksts" />
    <ShellContent
        Title="Assignement pievienošana"
        ContentTemplate="{DataTemplate local:AddAssignement}"
        Route="AddAssignement" />
    <ShellContent
        Title="Submission pievienošana"
        ContentTemplate="{DataTemplate local:AddSubmission}"
        Route="AddSubmission" />
    

</Shell>
